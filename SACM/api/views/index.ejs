<!-- Pagina de registro de pacientes sin comentarios -->

<html>
<head>
    <title>Registro de pacientes</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
</head>
<body>
    <div class="jumbotron text-center">
        <h1 class="text-center">Registro de pacientes</h1>
    </div>
    <div class="container" >
        <form class="form-control" method="post" action="/api/v1/pacientes">
            <label for="name">Nombre:</label>
        <input type="text" id="nombre" name="nombre" required><br>

        <label for="lastName">Apellido:</label>
        <input type="text" id="apellido" name="apellido" required><br>

        <label for="rut">RUT:</label>
        <input type="text" id="rut" name="rut" required><br>

        <label for="email">Email:</label>
        <input type="email" id="correo" name="correo" required><br>

        <label for="password">Contraseña:</label>
        <input type="password" id="contrasena" name="contrasena" required><br>

        <label for="address">Dirección:</label>
        <input type="text" id="address" name="address" required><br>

        <label for="comuna">Comuna:</label>
        <input type="text" id="comuna" name="comuna" required><br>

        <label for="region">Región:</label>
        <input type="text" id="region" name="region" required><br>

        <label for="telefono">Teléfono:</label>
        <input type="tel" id="telefono" name="telefono" required><br>

        <label for="whatsapp">WhatsApp:</label>
        <input type="tel" id="whatsapp" name="whatsapp"><br>

        <label for="celular">Celular:</label>
        <input type="tel" id="celular" name="celular" required><br>

        <label for="telefonoFamiliar">Teléfono Familiar:</label>
        <input type="tel" id="telefonoFamiliar" name="telefonoFamiliar"><br>

        <input type="submit" value="Enviar">
        </form>


    </div>
</body>
</html>